{
  "compilerOptions": {
    "strictNullChecks": true,
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/hamburger.jsx", "components/header.js", "pages/_app.jsx", "pages/_app.js", "pages/_document.js", "pages/solutions.js", "pages/opportunities.js", "pages/index.js", "pages/contactUs.js"],
  "exclude": ["node_modules"]
}
